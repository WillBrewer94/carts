function solid (x, y)
	--check for ground
	
	--coin
	if(fget(mget(x,y+offset),0)) then
		mset(x,y+offset,0)
	end
	
	if (x < 0 or x >= 128 ) then
		return true end
				
	val = mget(x, y+offset)
	return fget(val, 1)
end
